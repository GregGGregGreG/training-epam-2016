<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:tools="http://schemas.android.com/tools"
       xmlns:app="http://schemas.android.com/apk/res-auto"
       xmlns:android="http://schemas.android.com/apk/res/android"
       tools:showIn="@layout/activity_layouts">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/default_icon_size"
        android:background="?attr/colorPrimary"
        >

        <ImageButton
            android:onClick="onHamburgerIconClick"
            android:layout_width="@dimen/default_icon_size"
            android:layout_height="@dimen/default_icon_size"
            android:background="?attr/selectableItemBackground"
            app:srcCompat="@drawable/ic_reorder_white_24dp"
            android:layout_gravity="center_vertical"
            />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="I'm title"
            android:layout_marginLeft="@dimen/default_icon_size"
            android:layout_gravity="center_vertical"
            android:textColor="@android:color/white"/>

        <ImageButton
            android:onClick="onHamburgerIconClick"
            android:layout_width="@dimen/default_icon_size"
            android:layout_height="@dimen/default_icon_size"
            android:background="?attr/selectableItemBackground"
            app:srcCompat="@drawable/ic_reorder_white_24dp"
            android:layout_gravity="center_vertical|right"
            />
    </FrameLayout>

    <LinearLayout
        android:layout_marginTop="8dp"
        android:layout_width="match_parent"
        android:layout_height="@dimen/default_icon_size"
        android:background="?attr/colorPrimary"
        android:orientation="horizontal"
        >

        <ImageButton
            android:onClick="onHamburgerIconClick"
            android:layout_width="@dimen/default_icon_size"
            android:layout_height="@dimen/default_icon_size"
            android:background="?attr/selectableItemBackground"
            app:srcCompat="@drawable/ic_reorder_white_24dp"
            />

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="0.7"
            android:text="I'm title"
            android:layout_gravity="center_vertical"
            android:textColor="@android:color/white"/>

        <TextView
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="0.3"
            android:text="I'm second title"
            android:layout_gravity="center_vertical"
            android:textColor="@android:color/white"/>

        <ImageButton
            android:onClick="onHamburgerIconClick"
            android:layout_width="@dimen/default_icon_size"
            android:layout_height="@dimen/default_icon_size"
            android:background="?attr/selectableItemBackground"
            app:srcCompat="@drawable/ic_reorder_white_24dp"
            />
    </LinearLayout>

    <RelativeLayout
        android:layout_marginTop="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:orientation="horizontal"
        >

        <ImageButton
            android:id="@+id/btn1"
            android:onClick="onHamburgerIconClick"
            android:layout_width="@dimen/default_icon_size"
            android:layout_height="@dimen/default_icon_size"
            android:background="?attr/selectableItemBackground"
            app:srcCompat="@drawable/ic_reorder_white_24dp"
            />

        <TextView
            android:id="@+id/text1"
            android:layout_toRightOf="@id/btn1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dip"
            android:text="I'm title"
            android:textColor="@android:color/white"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="I'm second title"
            android:layout_below="@id/text1"
            android:layout_toRightOf="@id/btn1"
            android:textColor="@android:color/white"/>

        <ImageButton
            android:onClick="onHamburgerIconClick"
            android:layout_width="@dimen/default_icon_size"
            android:layout_height="@dimen/default_icon_size"
            android:layout_alignParentRight="true"
            android:background="?attr/selectableItemBackground"
            app:srcCompat="@drawable/ic_reorder_white_24dp"
            />
    </RelativeLayout>
</merge>